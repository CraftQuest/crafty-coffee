{
  "require": {
    "am-impact/amcommand": "^3.1",
    "craftcms/cms": "3.4.22.1",
    "craftcms/commerce": "3.1.5",
    "craftcms/commerce-stripe": "2.3.0",
    "craftcms/feed-me": "4.2.3",
    "craftcms/redactor": "2.6.1",
    "nystudio107/craft-seomatic": "3.3.5",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
